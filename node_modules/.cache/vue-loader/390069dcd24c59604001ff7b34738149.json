\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Nav from \"@/components/Nav.vue\";\r\nimport { reactive, toRefs } from \"vue\";\r\nimport { useStore } from \"vuex\";\r\nimport { Toast } from \"vant\";\r\nimport api from \"@/api/index\";\r\nimport { useRouter } from \"vue-router\";\r\n\r\nexport default {\r\n  components: { Nav },\r\n  name: \"UpdatePerson\",\r\n  setup() {\r\n    const store = useStore(),\r\n      router = useRouter();\r\n\r\n    let { name } = store.state.info;\r\n    let state = reactive({\r\n      name,\r\n      pic: [],\r\n      loading: false,\r\n    });\r\n\r\n    const submit = async () => {\r\n      if (state.loading) return;\r\n      state.loading = true;\r\n\r\n      let { name, pic } = state,\r\n        reg = /^[\\w\\u4e00-\\u9fa5]+$/;\r\n      // 格式校验\r\n      if (!reg.test(name)) {\r\n        Toast(\"小主，用户名输入错误~\");\r\n        return;\r\n      }\r\n      if (pic.length === 0) {\r\n        Toast(\"小主，必须上传一张图片~\");\r\n        return;\r\n      }\r\n      // 提交数据给服务器\r\n      let { code, data } = await api.userUpdate(name, pic[0].file);\r\n      if (+code !== 1) {\r\n        Toast(\"小主，信息修改失败，请稍后再试~\");\r\n        return;\r\n      }\r\n      Toast(\"小主太棒啦，信息修改成功~\");\r\n      store.commit(\"changeInfo\", data);\r\n      router.replace(\"/person\");\r\n\r\n      state.loading = false;\r\n    };\r\n\r\n    return {\r\n      ...toRefs(state),\r\n      submit,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.form-box {\r\n  margin-top: 30px;\r\n}\r\n</style>"]}]}