ACA,OADA;AACS,cAAA,WADT,yBACS,WADT;AAEZ,cAAA,KAAK,CAAC,KAAN,GAAc,IAAd;AACA,cAAA,KAAK,CAAC,QAAN,CAAe,IAAf,CACE,MAAM,CAAC,MAAP,CAAc;AACZ,gBAAA,IAAI,EAAJ,IADY;AAEZ,gBAAA,OAAM,EAAN;AAFY,eAAd,CADF;AAMA,cAAA,KAAK,CAAC,UAAN,GAAmB,MAAM,CAAC,MAAP,CAAc,WAAd,CAAnB,CATY,CASkC;;AATlC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAD,GAAb,CARM,CAmBN;;AACA,IAAA,SAAS,CAAC,YAAM;AACd,UAAI,EAAC,GAAI,IAAI,oBAAJ;AAAA,4EAAyB,kBAAM,OAAN;AAAA;AAAA;AAAA;AAAA;AAAA;AAC5B,kBAAA,IAD4B,GACrB,OAAO,CAAC,CAAD,CADc,EAEhC;;AAFgC,uBAG5B,IAAI,CAAC,cAHuB;AAAA;AAAA;AAAA;;AAAA;AAAA,yBAIX,GAAG,CAAC,eAAJ,CACjB,KAAK,CAAC,QAAN,CAAe,KAAK,CAAC,QAAN,CAAe,MAAf,GAAwB,CAAvC,EAA0C,MAA1C,CADiB,CAJW;;AAAA;AAI1B,kBAAA,MAJ0B;AAO9B,kBAAA,KAAK,CAAC,QAAN,CAAe,IAAf,CAAoB,MAAM,CAAC,MAAP,CAAc,MAAd,CAApB;;AAP8B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAzB;;AAAA;AAAA;AAAA;AAAA,UAAT;AAUA,MAAA,EAAE,CAAC,OAAH,CAAW,QAAQ,CAAC,KAApB,EAXc,CAWa;AAC5B,KAZQ,CAAT;AAaA,2CACK,MAAM,CAAC,KAAD,CADX;AAEE,MAAA,QAAQ,EAAR,QAFF;AAGE,MAAA,UAAS,EAAT;AAHF;AAKF;AA5Ca,CAAf","sourcesContent":["<template>\r\n  <!-- 头部 -->\r\n  <Head :time=\"today\"></Head>\r\n  <!-- 轮播图 -->\r\n  <section class=\"banner-box\">\r\n    <van-swipe v-if=\"bannerList.length > 0\" autoplay=\"3000\" lazy-render>\r\n      <van-swipe-item v-for=\"item in bannerList\" :key=\"item.id\">\r\n        <router-link :to=\"`/detail/${item.id}`\" class=\"content\">\r\n          <img :src=\"item.image\" alt=\"\" />\r\n          <div class=\"mark\">\r\n            <h3 class=\"title\">{{ item.title }}</h3>\r\n            <span class=\"tip\">{{ item.hint }}</span>\r\n          </div>\r\n        </router-link>\r\n      </van-swipe-item>\r\n    </van-swipe>\r\n  </section>\r\n  <!-- 新闻列表 -->\r\n  <van-skeleton title :row=\"5\" v-if=\"newsList.length === 0\"></van-skeleton>\r\n  <div v-else>\r\n    <section class=\"news-box\" v-for=\"(item, index) in newsList\" :key=\"index\">\r\n      <van-divider content-position=\"left\" v-if=\"index !== 0\">\r\n        {{ formatTime(item.date, '{1}月{2}日') }}\r\n      </van-divider>\r\n      <div class=\"content\">\r\n        <Item\r\n          v-for=\"item2 in item.stories\"\r\n          :key=\"item2.id\"\r\n          :data=\"item2\"\r\n        ></Item>\r\n      </div>\r\n    </section>\r\n  </div>\r\n  <div class=\"lazy-more\" v-show=\"newsList.length !== 0\" ref=\"loadMore\">\r\n    <van-loading size=\"12px\">小主，精彩数据准备中.....</van-loading>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, toRefs, onBeforeMount, ref, onMounted } from 'vue'\r\nimport Head from '../components/Head.vue'\r\nimport Item from '../components/Item.vue'\r\nimport api from '../api/index'\r\nimport { formatTime } from '../assets/utils'\r\nexport default {\r\n  name: 'Home',\r\n  components: {\r\n    Head,\r\n    Item\r\n  },\r\n  setup() {\r\n    let state = reactive({\r\n      today: '', //日期\r\n      newsList: [], //新闻列表\r\n      bannerList: [] //轮播图信息\r\n    })\r\n    let loadMore = ref(null)\r\n    // 在第一次渲染之前获取数据：今天日期 ，轮播图数据， 今日新闻\r\n    onBeforeMount(async () => {\r\n      let { date, stories, top_stories } = await api.queryNewsLatest()\r\n      state.today = date\r\n      state.newsList.push(\r\n        Object.freeze({\r\n          date,\r\n          stories\r\n        })\r\n      )\r\n      state.bannerList = Object.freeze(top_stories) //freeze冻结的元素不做代理，改变数据，组件不会重新渲染\r\n    })\r\n    // 第一次渲染完：加载更多数据\r\n    onMounted(() => {\r\n      let ob = new IntersectionObserver(async changes => {\r\n        let item = changes[0]\r\n        // 如果目标元素与交叉区域观察者对象 (intersection observer) 的根相交，则返回 true\r\n        if (item.isIntersecting) {\r\n          let result = await api.queryNewsBefore(\r\n            state.newsList[state.newsList.length - 1]['date']\r\n          )\r\n          state.newsList.push(Object.freeze(result))\r\n        }\r\n      })\r\n      ob.observe(loadMore.value) //observe可以指定观察哪个DOM元素\r\n    })\r\n    return {\r\n      ...toRefs(state),\r\n      loadMore,\r\n      formatTime\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n  .banner-box {\r\n    box-sizing: border-box;\r\n    height: 375px;\r\n    background-color: #d5d5d5;\r\n\r\n    .van-swipe {\r\n      height: 100%;\r\n      background-color: #fff;\r\n\r\n      .content {\r\n        position: relative;\r\n        display: block;\r\n        height: 100%;\r\n        color: #fff;\r\n\r\n        img {\r\n          display: block;\r\n          width: 100%;\r\n          height: 100%;\r\n        }\r\n\r\n        .mark {\r\n          position: absolute;\r\n          bottom: 0;\r\n          left: 0;\r\n          box-sizing: border-box;\r\n          padding: 10px 20px;\r\n          width: 100%;\r\n          height: 110px;\r\n          background: rgba(0, 0, 0, 0.3);\r\n          background: -webkit-linear-gradient(\r\n            top,\r\n            rgba(0, 0, 0, 0),\r\n            rgba(0, 0, 0, 0.3)\r\n          );\r\n\r\n          .title {\r\n            max-height: 50px;\r\n            font-size: 20px;\r\n            line-height: 25px;\r\n            overflow: hidden;\r\n          }\r\n\r\n          .tip {\r\n            font-size: 12px;\r\n            line-height: 30px;\r\n          }\r\n        }\r\n      }\r\n\r\n      /deep/ .van-swipe__indicators {\r\n        left: auto;\r\n        right: 15px;\r\n        transform: none;\r\n\r\n        .van-swipe__indicator--active {\r\n          width: 15px;\r\n          border-radius: 3px;\r\n          background: #fff;\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .news-box {\r\n    padding: 0 15px;\r\n\r\n    .van-divider {\r\n      margin: 5px 0;\r\n      font-size: 12px;\r\n\r\n      &::before {\r\n        display: none;\r\n      }\r\n    }\r\n  }\r\n  .van-skeleton {\r\n    padding: 20px 15px;\r\n  }\r\n  .lazy-more {\r\n    display: flex;\r\n    justify-content: center;\r\n    padding: 10px;\r\n    background-color: #f4f4f4;\r\n  }\r\n</style>\r\n"],"sourceRoot":""}]}