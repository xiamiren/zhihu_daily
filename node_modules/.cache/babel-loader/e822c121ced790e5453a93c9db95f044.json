e.dispatch('changeAsyncInfo')\r\n      }\r\n    })\r\n\r\n    return {\r\n      signout,\r\n      info\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.base-info {\r\n  box-sizing: border-box;\r\n  margin: 20px 0;\r\n\r\n  img {\r\n    display: block;\r\n    margin: 0 auto;\r\n    width: 86px;\r\n    height: 86px;\r\n    border-radius: 50%;\r\n  }\r\n\r\n  p {\r\n    line-height: 50px;\r\n    font-size: 18px;\r\n    text-align: center;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}