Z0B,MAAA,GAAG,EAAE,IAAI,CAAC;AAYpC,KAZjB,E;AAIa,MAAA,KAAK,EAAA,QAAA,CACd;AAAA,eAKE,CALF,YAAA,CAKE,qBALF,EAKE;AAJA,UAAA,MAAM,EAAN,EAIA;AAHA,UAAA,IAAI,EAAC,QAGL;AAFA,UAAA,IAAI,EAAC,IAEL;AADC,UAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,mBAAE,MAAA,CAAA,YAAA,CAAa,IAAI,CAAC,EAAlB,CAAF;AAAA;AACN,SALF,E,IAAA,E;;AAAA,U,WAAA,CAKE,CALF;AAAA,OADc,C;wBAHhB;AAAA,eAEM,CAFN,mBAAA,CAEM,KAFN,EAAA,UAAA,EAEM,CADJ,YAAA,CAAsE,eAAtE,EAAsE;AAA/D,UAAA,IAAI,kCAAO,IAAI,CAAC,IAAZ;AAAgB,YAAA,MAAA,EAAA,CAAW,IAAI,CAAC,IAAL,CAAU,KAArB,CAAhB;AAA0C,YAAA,IAAA,EAAA;AAA1C;AAA2D,SAAtE,E,IAAA,E;;AAAA,U,QAAA,CACI,CAFN,CAEM,CAFN;AAAA,O;;;;KADF,E;;AAAA,K;GAYiB,CAZjB,E;;AAAA,GAaI,EAdN,C","sourcesContent":["<template>\r\n  <Nav title=\"收藏列表\" />\r\n  <van-empty description=\"暂无收藏\" v-if=\"storeList.length === 0\" />\r\n  <div v-else>\r\n    <van-swipe-cell v-for=\"item in storeList\" :key=\"item.id\">\r\n      <div class=\"con\">\r\n        <Item :data=\"{ ...item.news, images: [item.news.image], hint: '' }\" />\r\n      </div>\r\n      <template #right>\r\n        <van-button\r\n          square\r\n          type=\"danger\"\r\n          text=\"删除\"\r\n          @click=\"removeHandle(item.id)\"\r\n        />\r\n      </template>\r\n    </van-swipe-cell>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Nav from \"@/components/Nav.vue\";\r\nimport Item from \"@/components/Item.vue\";\r\nimport { computed, onBeforeMount } from \"vue\";\r\nimport { useStore } from \"vuex\";\r\nimport api from \"@/api/index\";\r\nimport { Toast } from \"vant\";\r\n\r\nexport default {\r\n  components: { Nav, Item },\r\n  name: \"Store\",\r\n  setup() {\r\n    const store = useStore();\r\n    // vuex中获取storeList\r\n    let storeList = computed(() => {\r\n      return store.state.storeList || [];\r\n    });\r\n    // 渲染之前将store数据存储到vuex中\r\n    onBeforeMount(() => {\r\n      if (store.state.storeList === null) {\r\n        store.dispatch(\"changeStoreListAsync\");\r\n      }\r\n    });\r\n    const removeHandle = (id) => {\r\n      // 发送请求删除后台的数据\r\n      let code = api.storeRemove(id);\r\n      if (+code !== 0) {\r\n        Toast(\"很遗憾，删除失败\");\r\n        return;\r\n      }\r\n      Toast(\"小主很棒，删除成功\");\r\n      //更新vuex中的数据\r\n      store.commit('removeStoreList', id);\r\n    };\r\n    \r\n    return {\r\n      storeList,\r\n      removeHandle,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.van-swipe-cell {\r\n  .con {\r\n    padding: 0 15px;\r\n  }\r\n\r\n  /deep/ .van-swipe-cell__right {\r\n    .van-button {\r\n      position: relative;\r\n      top: 50%;\r\n      transform: translateY(-50%);\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}